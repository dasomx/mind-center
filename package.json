{
	"name": "mind-center",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"dev": "vite dev",
		"emulator": "firebase emulators:start --import firebase.data",
		"emulator:export": "firebase emulators:export ./firebase.data",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@event-calendar/core": "^3.4.0",
		"@event-calendar/day-grid": "^3.4.0",
		"@event-calendar/time-grid": "^3.4.0",
		"@material/typography": "^14.0.0",
		"@mdi/js": "^7.4.47",
		"@playwright/test": "^1.28.1",
		"@smui-extra/autocomplete": "^7.0.0",
		"@smui/button": "^7.0.0-beta.18",
		"@smui/card": "^7.0.0",
		"@smui/circular-progress": "^7.0.0",
		"@smui/common": "^7.0.0-beta.18",
		"@smui/data-table": "^7.0.0",
		"@smui/drawer": "^7.0.0",
		"@smui/form-field": "^7.0.0-beta.18",
		"@smui/icon-button": "^7.0.0",
		"@smui/layout-grid": "^7.0.0",
		"@smui/linear-progress": "^7.0.0",
		"@smui/list": "^7.0.0",
		"@smui/menu": "^7.0.0-beta.18",
		"@smui/paper": "^7.0.0",
		"@smui/radio": "^7.0.0",
		"@smui/select": "^7.0.0",
		"@smui/snackbar": "^7.0.0",
		"@smui/tab": "^7.0.0",
		"@smui/tab-bar": "^7.0.0",
		"@smui/textfield": "^7.0.0-beta.18",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"carbon-components-svelte": "^0.85.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"firebase-admin": "^12.3.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"smui-theme": "^7.0.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^6.0.2",
		"tslib": "^2.4.1",
		"typescript": "^5.5.4",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@material/theme": "^14.0.0",
		"firebase": "^10.11.1",
		"svelte-material-ui": "^7.0.0-beta.18"
	}
}
